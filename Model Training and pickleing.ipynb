   "source": [
    "## Dividing the features and Target\n",
    "x=df.drop('HeartDisease',axis=1)\n",
    "y=df['HeartDisease']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 63,
   "id": "cb11684a",
   "metadata": {},
   "outputs": [],
   "source": [
    "## Splitting Training and Testing \n",
    "X_train,X_test,y_train,y_test=train_test_split(x,y,test_size=0.2,random_state=42,stratify=y)"
   ]
  },
  {
